{
  "description": "Live tamper your web. Use existing userscripts or develop your own. Jack your  editor into any web page to tamper with it in real-time. Or let your AI agent hack the page while you are browsing.",
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "scripting",
    "activeTab",
    "storage",
    "webNavigation"
  ],
  "name": "Scittle Tamper",
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "devtools_page": "devtools.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon-16.png",
      "32": "icons/icon-32.png",
      "48": "icons/icon-48.png",
      "128": "icons/icon-128.png"
    }
  },
  "manifest_version": 3,
  "version": "0.0.7.110",
  "web_accessible_resources": [
    {
      "resources": [
        "vendor/scittle.js",
        "vendor/scittle.nrepl.js",
        "ws-bridge.js",
        "trigger-scittle.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}